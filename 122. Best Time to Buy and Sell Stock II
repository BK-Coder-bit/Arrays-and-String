122. Best Time to Buy and Sell Stock II

https://leetcode.com/problems/best-time-to-buy-and-sell-stock-ii/description/
class Solution(object):
    def maxProfit(self, prices): #O(n)& O(1)
        i=0
        lo=prices[0]
        hi=prices[0]
        profit=0
        n=len(prices)
        while i< n-1:
            #look where to buy 
            while i<n-1 and prices[i]>=prices[i+1]:
                i+=1
            lo=prices[i]
            #look where to sell
            while i<n-1 and prices[i]<=prices[i+1]:
                i+=1
            hi=prices[i]
            profit+=hi-lo
        return profit
